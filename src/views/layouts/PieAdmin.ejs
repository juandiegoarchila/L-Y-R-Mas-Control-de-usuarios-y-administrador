<!-- menu desplegable -->
<script>
    document.addEventListener('DOMContentLoaded', function () {
        let dropdownButton = document.querySelector('.dropbtn');
        let dropdownContent = document.getElementById('dropdownContent');

        // Agrega un controlador de eventos para hacer clic en el botón
        dropdownButton.addEventListener('click', function (event) {
            // Detén la propagación del evento para evitar cerrar inmediatamente el menú
            event.stopPropagation();
            
            // Alternar la visibilidad del menú desplegable
            dropdownContent.style.display = (dropdownContent.style.display === 'block') ? 'none' : 'block';
        });

        // Agrega un controlador de eventos al cuerpo del documento para cerrar el menú cuando se hace clic fuera de él
        document.body.addEventListener('click', function () {
            dropdownContent.style.display = 'none';
        });

        // Agrega un controlador de eventos para evitar que el clic en el menú se propague al cuerpo del documento
        dropdownContent.addEventListener('click', function (event) {
            event.stopPropagation();
        });
    });

    
</script>
